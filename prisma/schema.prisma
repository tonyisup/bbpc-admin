generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
}

model Episode {
  id                  String                @id @default(dbgenerated("newid()")) @db.UniqueIdentifier
  number              Int                   @db.SmallInt
  title               String
  recording           String?
  date                DateTime?             @db.Date
  description         String?
  status              String?               @default("published")
  Assignments         Assignment[]
  AudioEpisodeMessage AudioEpisodeMessage[]
  Bangers             Banger[]
  Extras              ExtraReview[]
  Links               Link[]
}

model Link {
  id        String   @id @default(dbgenerated("newid()")) @db.UniqueIdentifier
  url       String
  text      String
  episodeId String?  @db.UniqueIdentifier
  Episode   Episode? @relation(fields: [episodeId], references: [id], onDelete: Cascade)
}

model Movie {
  id          String       @id @default(dbgenerated("newid()")) @db.UniqueIdentifier
  title       String
  year        Int          @db.SmallInt
  poster      String?
  url         String
  Assignments Assignment[]
  Reviews     Review[]
  Syllabus    Syllabus[]
}

model Show {
  id      String   @id @default(dbgenerated("newid()")) @db.UniqueIdentifier
  title   String
  year    Int      @db.SmallInt
  poster  String?
  url     String
  Reviews Review[]
}

model Rating {
  id       String   @id @default(dbgenerated("newid()")) @db.UniqueIdentifier
  name     String
  value    Int      @db.TinyInt
  sound    String?
  icon     String?
  category String?
  Guesses  Guess[]
  Reviews  Review[]
}

model Review {
  id                String             @id @default(dbgenerated("newid()")) @db.UniqueIdentifier
  userId            String?
  movieId           String?            @db.UniqueIdentifier
  showId            String?            @db.UniqueIdentifier
  ratingId          String?            @db.UniqueIdentifier
  ReviewdOn         DateTime?          @default(now(), map: "DF__Review__ReviewdO__0C1BC9F9") @db.DateTime
  AssignmentReviews AssignmentReview[]
  ExtraReviews      ExtraReview[]
  Movie             Movie?             @relation(fields: [movieId], references: [id], onUpdate: NoAction, onDelete: NoAction)
  Show              Show?              @relation(fields: [showId], references: [id], onUpdate: NoAction, onDelete: NoAction)
  Rating            Rating?            @relation(fields: [ratingId], references: [id], onUpdate: NoAction, onDelete: NoAction)
  User              User?              @relation(fields: [userId], references: [id], onUpdate: NoAction, onDelete: Cascade)
}

model ExtraReview {
  id        String  @id @default(dbgenerated("newid()")) @db.UniqueIdentifier
  reviewId  String  @db.UniqueIdentifier
  episodeId String  @db.UniqueIdentifier
  Episode   Episode @relation(fields: [episodeId], references: [id], onUpdate: NoAction, onDelete: NoAction)
  Review    Review  @relation(fields: [reviewId], references: [id], onUpdate: NoAction, onDelete: NoAction)
}

model AssignmentReview {
  id           String     @id @default(dbgenerated("newid()")) @db.UniqueIdentifier
  assignmentId String     @db.UniqueIdentifier
  reviewId     String     @db.UniqueIdentifier
  Assignment   Assignment @relation(fields: [assignmentId], references: [id], onUpdate: NoAction, onDelete: NoAction)
  Review       Review     @relation(fields: [reviewId], references: [id], onUpdate: NoAction, onDelete: NoAction)
  Guesses      Guess[]
}

model Assignment {
  id                String             @id @default(dbgenerated("newid()")) @db.UniqueIdentifier
  userId            String
  episodeId         String             @db.UniqueIdentifier
  movieId           String             @db.UniqueIdentifier
  type              String             @default("HOMEWORK")
  Episode           Episode            @relation(fields: [episodeId], references: [id])
  Movie             Movie              @relation(fields: [movieId], references: [id])
  User              User               @relation(fields: [userId], references: [id], onUpdate: NoAction, onDelete: NoAction)
  AssignmentReviews AssignmentReview[]
  AudioMessage      AudioMessage[]
  GamblingPoints    GamblingPoints[]
  Syllabus          Syllabus[]
}

model Banger {
  id        String   @id @default(dbgenerated("newid()")) @db.UniqueIdentifier
  title     String
  artist    String
  url       String
  episodeId String?  @db.UniqueIdentifier
  userId    String?
  Episode   Episode? @relation(fields: [episodeId], references: [id], onDelete: Cascade)
  User      User?    @relation(fields: [userId], references: [id])
}

model Role {
  id          Int        @id @default(autoincrement()) @db.TinyInt
  name        String
  description String
  admin       Boolean    @default(false)
  UserRoles   UserRole[]
}

model UserRole {
  id     String @id @default(dbgenerated("newid()")) @db.UniqueIdentifier
  userId String
  roleId Int    @db.TinyInt
  Role   Role   @relation(fields: [roleId], references: [id], onUpdate: NoAction, onDelete: NoAction)
  User   User   @relation(fields: [userId], references: [id], onUpdate: NoAction, onDelete: Cascade)
}

model Account {
  id                String  @id @default(cuid())
  userId            String
  type              String
  provider          String
  providerAccountId String
  refresh_token     String? @db.Text
  access_token      String? @db.Text
  expires_at        Int?
  token_type        String?
  scope             String?
  id_token          String? @db.Text
  session_state     String?
  user              User    @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@unique([provider, providerAccountId])
}

model Session {
  id           String   @id @default(cuid())
  sessionToken String   @unique
  userId       String
  expires      DateTime
  user         User     @relation(fields: [userId], references: [id], onDelete: Cascade)
}

model VerificationToken {
  identifier String
  token      String   @unique
  expires    DateTime

  @@unique([identifier, token])
}

model User {
  id                  String                @id @default(cuid())
  name                String?
  email               String?               @unique
  emailVerified       DateTime?
  image               String?
  Accounts            Account[]
  Assignments         Assignment[]
  AudioEpisodeMessage AudioEpisodeMessage[]
  AudioMessage        AudioMessage[]
  Bangers             Banger[]
  GamblingPoints      GamblingPoints[]
  Guesses             Guess[]
  Point               Point[]
  Reviews             Review[]
  Sessions            Session[]
  Syllabus            Syllabus[]
  UserRoles           UserRole[]
}

model Guess {
  id                String           @id @default(dbgenerated("newid()")) @db.UniqueIdentifier
  ratingId          String           @db.UniqueIdentifier
  created           DateTime         @db.DateTime
  userId            String
  assignmntReviewId String           @db.UniqueIdentifier
  seasonId          String           @db.UniqueIdentifier
  pointsId          String?          @db.UniqueIdentifier
  Point             Point?           @relation(fields: [pointsId], references: [id], onUpdate: NoAction, onDelete: NoAction)
  AssignmentReview  AssignmentReview @relation(fields: [assignmntReviewId], references: [id], onUpdate: NoAction, onDelete: NoAction)
  Rating            Rating           @relation(fields: [ratingId], references: [id], onUpdate: NoAction, onDelete: NoAction)
  Season            Season           @relation(fields: [seasonId], references: [id], onUpdate: NoAction, onDelete: NoAction)
  User              User             @relation(fields: [userId], references: [id], onUpdate: NoAction, onDelete: Cascade)
}

model Season {
  id             String           @id @default(dbgenerated("newid()")) @db.UniqueIdentifier
  title          String
  description    String?
  gameTypeId     Int              @db.TinyInt
  endedOn        DateTime?        @db.DateTime
  startedOn      DateTime?        @db.DateTime
  Guesses        Guess[]
  Point          Point[]
  GamblingPoints GamblingPoints[]
  GameType       GameType         @relation(fields: [gameTypeId], references: [id], onDelete: Cascade)
}

model GameType {
  id          Int         @id @default(autoincrement()) @db.TinyInt
  title       String
  description String?
  Seasons     Season[]
  GamePoint   GamePoint[]
}

model GamePoint {
  id          Int      @id @default(autoincrement()) @db.TinyInt
  lookupID    String   @unique
  title       String
  description String?
  points      Int      @db.SmallInt
  gameTypeId  Int      @db.TinyInt
  Point       Point[]
  GameType    GameType @relation(fields: [gameTypeId], references: [id], onDelete: Cascade)
}

model Point {
  id             String           @id @default(dbgenerated("newid()")) @db.UniqueIdentifier
  userId         String
  seasonId       String           @db.UniqueIdentifier
  adjustment     Int              @default(0) @db.SmallInt
  reason         String
  earnedOn       DateTime         @db.DateTime
  gamePointId    Int?             @db.TinyInt
  GamePoint      GamePoint?       @relation(fields: [gamePointId], references: [id], onDelete: Cascade)
  GamblingPoints GamblingPoints[]
  Guess          Guess[]
  Season         Season           @relation(fields: [seasonId], references: [id], onUpdate: NoAction, onDelete: NoAction)
  User           User             @relation(fields: [userId], references: [id], onUpdate: NoAction, onDelete: Cascade)
}

model AudioMessage {
  id           Int         @id @default(autoincrement())
  url          String
  createdAt    DateTime    @default(now())
  userId       String
  assignmentId String?     @db.UniqueIdentifier
  fileKey      String?
  Assignment   Assignment? @relation(fields: [assignmentId], references: [id], onDelete: NoAction, onUpdate: NoAction)
  User         User        @relation(fields: [userId], references: [id], onUpdate: Cascade)
}

model AudioEpisodeMessage {
  id        Int      @id @default(autoincrement())
  url       String
  createdAt DateTime @default(now())
  fileKey   String?
  userId    String
  episodeId String?  @db.UniqueIdentifier
  Episode   Episode? @relation(fields: [episodeId], references: [id], onDelete: NoAction, onUpdate: NoAction)
  User      User     @relation(fields: [userId], references: [id], onUpdate: Cascade)
}

model GamblingPoints {
  id           String      @id @default(dbgenerated("newid()")) @db.UniqueIdentifier
  userId       String
  assignmentId String      @db.UniqueIdentifier
  seasonId     String?     @db.UniqueIdentifier
  pointsId     String?     @db.UniqueIdentifier
  points       Int
  createdAt    DateTime    @default(now()) @db.DateTime
  successful   Boolean?    @default(false)
  Assignment   Assignment? @relation(fields: [assignmentId], references: [id], onUpdate: NoAction, onDelete: NoAction)
  User         User        @relation(fields: [userId], references: [id], onUpdate: NoAction, onDelete: Cascade)
  Season       Season?     @relation(fields: [seasonId], references: [id], onUpdate: NoAction, onDelete: NoAction)
  Point        Point?      @relation(fields: [pointsId], references: [id], onUpdate: NoAction, onDelete: NoAction)
}

model Syllabus {
  id           String      @id @default(dbgenerated("newid()")) @db.UniqueIdentifier
  userId       String
  movieId      String      @db.UniqueIdentifier
  order        Int
  createdAt    DateTime    @default(now()) @db.DateTime
  assignmentId String?     @db.UniqueIdentifier
  notes        String?
  Assignment   Assignment? @relation(fields: [assignmentId], references: [id], onDelete: NoAction, onUpdate: NoAction)
  Movie        Movie       @relation(fields: [movieId], references: [id], onDelete: Cascade, onUpdate: NoAction)
  User         User        @relation(fields: [userId], references: [id], onDelete: Cascade, onUpdate: Cascade)
}
